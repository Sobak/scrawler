{
    "name": "sobak/scrawler",
    "description": "Scrawler is an extensible, declarative web robot and scrapper",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.2.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/console": "^4.2",
        "symfony/css-selector": "^4.2",
        "symfony/dom-crawler": "^4.2"
    },
    "require-dev": {
        "doctrine/orm": "^2.6",
        "phpunit/phpunit": "^8.0",
        "symfony/process": "^4.2",
        "symfony/var-dumper": "^4.2"
    },
    "suggest": {
        "doctrine/orm": "Required to use DatabaseResultWriter"
    },
    "autoload": {
        "psr-4": {
            "Sobak\\Scrawler\\": "src/"
        },
        "files": [
            "src/Support/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "dev/app/",
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Maciej Sobaczewski",
            "homepage": "http://sobak.pl"
        }
    ],
    "bin": [
        "bin/scrawler"
    ],
    "scripts": {
        "tests": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
