{
    "name": "sobak/scrawler",
    "description": "Scrawler is an extensible, declarative web robot and scrapper",
    "type": "library",
    "require": {
        "php": ">=7.2.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/console": "^4.2",
        "symfony/css-selector": "^4.2",
        "symfony/dom-crawler": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "symfony/process": "^4.2",
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Sobak\\Scrawler\\": "src/"
        },
        "files": [
            "src/Support/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "dev/app/",
            "Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/scrapper"
    ],
    "scripts": {
        "tests": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
